<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <TextView
                style="@style/FFC.TextView.Title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/visitancdelivcer"/>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.pregno"/>

        <EditText
                android:id="@id/answer1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:digits="0123456789"
                android:editable="false"
                android:hint="@string/hint_ppl_order"
                android:inputType="numberDecimal|numberSigned"
                android:maxLength="2"/>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.hosservice"/>

        <th.in.ffc.widget.SearchableButton
                android:id="@id/answer2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.hosservice"/>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.datedeliver"/>

        <th.in.ffc.util.ThaiDatePicker
                android:id="@id/answer3"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">
        </th.in.ffc.util.ThaiDatePicker>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.delivertime"/>

        <th.in.ffc.util.ThaiTimePicker
                android:id="@id/answer4"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">
        </th.in.ffc.util.ThaiTimePicker>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.ancage"/>

        <TextView
                android:id="@+id/abortionLength"
                style="@style/FFC.TextView.Content"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dip"/>

        <TextView
                style="@style/FFC.TextView.Subject"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/visitancdeliver.deliverresult"/>

        <th.in.ffc.widget.ArrayFormatSpinner
                android:id="@id/answer5"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:prompt="@string/visitancdeliver.deliverresult"/>

