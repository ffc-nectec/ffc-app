                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.95"
                        android:checked="true"
                        android:text="@string/have"/>

                <RadioButton
                        android:id="@id/rad2"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/no_have"/>
            </RadioGroup>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.breast"/>

            <RadioGroup
                    android:id="@+id/answer24"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <RadioButton
                        android:id="@id/rad1"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.95"
                        android:checked="true"
                        android:text="@string/normal"/>

                <RadioButton
                        android:id="@id/rad2"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/blind"/>
            </RadioGroup>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.milk"/>

            <RadioGroup
                    android:id="@+id/answer25"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <RadioButton
