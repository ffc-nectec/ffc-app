                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.95"
                        android:checked="true"
                        android:text="@string/flow"/>

                <RadioButton
                        android:id="@id/rad2"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/no_flow"/>
            </RadioGroup>

