                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancdeliver.numdeadinpreg"/>

            <EditText
                    android:id="@id/answer8"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:digits="0123456789"
                    android:maxLength="2"
                    android:hint="@string/hint_ppl"
                    android:inputType="numberDecimal|numberSigned"/>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancdeliver.deliverend"/>

            <Spinner
                    android:id="@id/answer10"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:prompt="@string/visitancdeliver.deliverend"/>
