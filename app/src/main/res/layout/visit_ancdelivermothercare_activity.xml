                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.95"
                        android:checked="true"
                        android:text="@string/normal"/>

                <RadioButton
                        android:id="@id/rad2"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/strange"/>
            </RadioGroup>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.alb"/>

            <th.in.ffc.widget.ArrayFormatSpinner
                    android:id="@+id/answer27"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:prompt="@string/visitancmothercare.alb"/>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.sugar"/>

            <th.in.ffc.widget.ArrayFormatSpinner
                    android:id="@+id/answer28"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:prompt="@string/visitancmothercare.sugar"/>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.tear"/>

            <th.in.ffc.widget.ArrayFormatSpinner
                    android:id="@+id/answer29"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:prompt="@string/visitancmothercare.tear"/>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.ttno35"/>

            <Button
                    android:id="@+id/getViccine"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="@string/get_vaccine"/>

            <TextView
                    style="@style/FFC.TextView.Subject"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitancmothercare.resultcheck"/>

            <RadioGroup
                    android:id="@+id/answer30"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <RadioButton
                        android:id="@id/rad1"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.95"
                        android:checked="true"
                        android:text="@string/normal"/>

                <RadioButton
                        android:id="@id/rad2"
                        android:layout_width="0dip"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="@string/strange"/>
            </RadioGroup>
